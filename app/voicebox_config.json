{
    "model": {
        "name": "tts_models/multilingual/multi-dataset/xtts_v2",
        "device": "cuda"
    },
    "vocoder": {
        "speaker_wav": "data/voices/biden/biden_01.wav",
        "speed_up": 1.0,
        "apply_dynamic_compression": true,
        "apply_low_pass": false,
        "apply_high_pass": false,
        "threshold": -20,
        "ratio": 4.0,
        "attack": 5.0,
        "release": 50.0,
        "low_pass_cutoff": 4000,
        "high_pass_cutoff": 200
    },
    "synth_params": {
        "temperature": 0.7,
        "gpt_cond_len": 6,
        "top_k": 50,
        "top_p": 0.9,
        "decoder_iterations": 30
    },
    "silence_filter_params": {
        "silence_thresh": -40,
        "min_silence_len": 500,
        "keep_silence": 250
    }
} 